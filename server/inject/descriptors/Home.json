{
  "type": "object",
  "properties": {
    "isHome": {
      "type": "identifier",
      "selector": "h2",
      "control": "exists",
      "selectContains": "Welcome"
    },
    "getMyTimeTable": {
      "type": "action",
      "control": "action.link",
      "selector": "a",
      "selectContains": "My Time Table"
    }
  },
  "connect": [
    {
      "match": [
        "isHome"
      ],
      "action": "getMyTimeTable"
    }
  ]
}
