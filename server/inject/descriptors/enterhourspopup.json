{
  "type": "object",
  "properties": {
    "isEnterHoursPopup": {
      "type": "identifier",
      "selector": "#ui-dialog-title-dialog-form",
      "control": "exists",
      "selectContains": "Enter Hours in Week"
    },
    "customer": {
      "type": "string",
      "control": "text",
      "selector": "#dcustomer"
    },
    "project": {
      "type": "string",
      "control": "text",
      "selector": "#dproject"
    },
    "function": {
      "type": "string",
      "control": "text",
      "selector": "#dfunction"
    },
    "hours": {
      "type": "string",
      "control": "text",
      "selector": "#hourAmount"
    },
    "clickConfirm": {
      "type": "action",
      "control": "action.button",
      "selector": ".ui-button.ui-widget.ui-state-default.ui-corner-all.ui-button-text-only"
    }
  },
  "connect": [
    {
      "match": [
        "isEnterHoursPopup"
      ],
      "screens": [
        "enterhourspopup"
      ]
    }
  ]
}
