{
  "type": "object",
  "properties": {
    "isMyTimeTable": {
      "type": "identifier",
      "control": "exists",
      "selector": "b",
      "selectContains": "Already Allocated"
    },
    "MyWeeks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Week": {
            "type": "string",
            "control": "property.innerText",
            "selector": "td:nth-child(2) > h2"
          },
          "Timeframe": {
            "type": "string",
            "selector": "td > p",
            "control": "property.innerText"
          },
          "AllocatedHours": {
            "type": "string",
            "selector": "td:nth-child(3)",
            "control": "property.innerText"
          },
          "ReleasedHours": {
            "type": "string",
            "selector": "td:nth-child(4)",
            "control": "property.innerText"
          },
          "TrackHours": {
            "type": "action",
            "selector": "#main > table > tbody > tr:nth-child(5) > td.ui-buttonset",
            "control": "action.button"
          }
        }
      },
      "selectorAll": "#main > table > tbody > tr"
    }
  },
  "connect": []
}
