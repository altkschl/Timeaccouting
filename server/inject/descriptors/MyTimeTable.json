{
  "type": "object",
  "properties": {
    "isMyTimeTable": {
      "type": "identifier",
      "control": "exists",
      "selector": "b",
      "selectContains": "Already Allocated"
    },
    "mytimetable": {
      "type": "array",
      "selectorAll": "#main > table > tbody > tr:nth-child(odd):not(:first-child)",
      "items": {
        "type": "object",
        "properties": {
          "week": {
            "type": "string",
            "selector": "td:nth-child(2) > h2",
            "control": "property.innerHTML"
          },
          "timeframe": {
            "type": "string",
            "selector": "td:nth-child(2) > p",
            "control": "property.innerHTML"
          },
          "allocatedhours": {
            "type": "string",
            "selector": "#main",
            "control": "property.innerHTML"
          },
          "releasedhours": {
            "type": "string",
            "selector": "#main",
            "control": "property.innerHTML"
          }
        }
      }
    }
  },
  "connect": []
}
